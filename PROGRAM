#include <bits/stdc++.h>
using namespace std;
 
void printVec(vector<int> v){
    // O(1)
    cout<<v.size()<<endl;
 
    // O(n)
    for(int i=0; i<v.size(); i++){
        cout<<v[i]<<" ";
    }
    cout<<endl;
}
 
